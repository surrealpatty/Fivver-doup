{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "decoratorMetadata": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2015"
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true,
  "env": {
    "targets": "node 16"
  },
  "exclude": ["node_modules"]
}
