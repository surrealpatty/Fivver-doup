  },
  "dependencies": {
    "axios": "^1.7.7",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.3.1",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^29.7.0",
