{
  "scripts": {
    "sequelize": "NODE_ENV=development ts-node --transpileOnly node_modules/.bin/sequelize-cli",
    "format": "prettier --write .",
    "clean-build": "rimraf dist && tsc",
    "dev": "ts-node-dev src/index.ts",
    "build": "swc src --out-dir dist --config-file .swcrc && cmd /c move dist\\src\\index.js dist\\index.js && npx tsc",
    "build:watch": "swc src -d dist --watch",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --silent --maxWorkers=2", 
    "test:watch": "jest --watch --silent", 
    "test:coverage": "jest --coverage --silent --detectOpenHandles", 
    "test:ci": "jest --ci --maxWorkers=4", 
    "lint": "eslint \"src/**/*.{ts,js,vue}\" --fix", 
    "lint:check": "eslint \"src/**/*.{ts,js,vue}\"", 
    "migrate": "npx sequelize-cli db:migrate", 
    "db:migrate": "ts-node -r tsconfig-paths/register node_modules/sequelize-cli/lib/sequelize.js db:migrate", 
    "seed": "npx sequelize-cli db:seed:all", 
    "prepublishOnly": "npm run build", 
    "prepare": "husky install", 
    "lint-staged": "lint-staged", 
    "clean": "rimraf dist/ build/", 
    "start": "node index.js"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.2.2",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
