{
  "scripts": {
    "sequelize": "NODE_ENV=development ts-node --transpileOnly node_modules/.bin/sequelize-cli",
    "format": "prettier --write .",
    "clean-build": "rimraf dist && tsc",
    "dev": "ts-node-dev src/index.ts",
    "build": "swc src --out-dir dist --config-file .swcrc && cmd /c move dist\\src\\index.js dist\\index.js && npx tsc",
    "build:watch": "swc src -d dist --watch",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:ci": "jest --ci --maxWorkers=4",
    "lint": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,js,vue}\"",
    "migrate": "npx sequelize-cli db:migrate",
    "db:migrate": "ts-node -r tsconfig-paths/register node_modules/sequelize-cli/lib/sequelize.js db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "clean": "rimraf dist/ build/",
    "start": "node index.js"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
