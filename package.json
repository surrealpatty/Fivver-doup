{
  "name": "fivver-doup",
  "version": "1.0.0",
  "description": "A Fiverr-like platform for skill-based posts.",
  "author": "Patty",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test DB_USER=testuser DB_PASSWORD=testpassword DB_NAME=fivver_doup_test DB_HOST=127.0.0.1 DB_PORT=3306 jest --config jest.config.ts --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:ci": "jest --ci --maxWorkers=4",
    "lint": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,js,vue}\"",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "prepare": "husky install"
  },
  "keywords": [
    "fiverr-clone",
    "skills",
    "platform"
  ],
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "react": "^18.3.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.0.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.16.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.16.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-express": "^0.1.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  },
  "eslintConfig": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "types": "dist/index.d.ts"
}
